{
	"name": "bot",
	"version": "1.0.0",
	"description": "Telegram Bot with Analytics and LLM Integration",
	"main": "dist/index.js",
	"scripts": {
		"dev": "ts-node src/index.ts",
		"dev:api": "ts-node src/api.ts",
		"build": "tsc",
		"start": "concurrently \"node dist/index.js\" \"node dist/api.js\"",
		"start:bot": "node dist/index.js",
		"start:api": "node dist/api.js",
		"test": "vitest"
	},
	"keywords": [
		"telegram",
		"bot",
		"analytics"
	],
	"author": "",
	"license": "ISC",
	"type": "commonjs",
	"dependencies": {
		"@google/generative-ai": "^0.3.0",
		"concurrently": "^8.2.2",
		"dotenv": "^16.3.1",
		"pg": "^8.11.3",
		"redis": "^4.6.12",
		"telegraf": "^4.16.3"
	},
	"devDependencies": {
		"@types/node": "^20.10.6",
		"@types/pg": "^8.11.2",
		"@vitest/ui": "^1.1.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"vitest": "^1.6.1"
	}
}
